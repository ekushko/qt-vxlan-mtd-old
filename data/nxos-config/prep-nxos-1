config t

vlan 2
  vn-segment 200

vlan 50
  vn-segment 5000
 
vlan 60
  vn-segment 6000

vlan 70
  vn-segment 7000

interface e1/2-7
  switchport
  switchport mode trunk
  switchport trunk allowed vlan 2
  no shutdown

interface nve 1
  no shutdown
  source-interface loopback 0
  member vni 200
   ingress-replication protocol static
     peer-ip 20.20.20.20
  member vni 5000
   ingress-replication protocol static
     peer-ip 20.20.20.20
  member vni 6000
   ingress-replication protocol static
     peer-ip 20.20.20.20
  member vni 7000
   ingress-replication protocol static
     peer-ip 20.20.20.20
